---
layoyt: null
---

<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title><%= escape_html(site.title) %></title>
    <description><%= escape_html(site.description) %></description>
    <link><%= site.url %><%= site.baseurl %>/</link>
    <atom:link href="<%= site.url + '/feed.xml' %>" rel="self" type="application/rss+xml" />
    <pubDate><%= time(site.time) %></pubDate>
    <lastBuildDate><%= time(site.time) %></lastBuildDate>
    <generator>Jekyll v<%= jekyll.version %></generator>
    <% for (post of site.posts.slice(0, 10)) { %>
      <item>
        <title><%= escape_html(post.title) %></title>
        <description><%= escape_html(bookmark_filter(post.content)) %></description>
        <pubDate><%= time(post.date) %></pubDate>
        <link><%= site.url + post.url %></link>
        <guid isPermaLink="true"><%= site.url + post.url %></guid>
        <% for (tag of post.tags) { %>
        <category><%= escape_html(tag) %></category>
        <% } %>
        <% for (cat of post.categories) { %>
        <category><%= escape_html(cat) %></category>
        <% } %>
      </item>
    <% } %>
  </channel>
</rss>
